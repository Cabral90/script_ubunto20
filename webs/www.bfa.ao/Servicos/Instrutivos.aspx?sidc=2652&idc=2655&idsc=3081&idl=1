<script type="text/javascript">
    var minSearchCharacters = 3;

    $(function() {
        initBody();
        stopLoading();
    });

    function bindDroplist() {
        var obj = '.drop-style';

        $(this).parents(obj).find('.drop-selected').text($(this).text());
        $(this).parents(obj).find('.drop-text').val($(this).text());
        $(this).parents(obj).find('.drop-value').val($(this).attr('id'));
        $(this).parent().slideUp();
    }

    function setDocOnInit() {
        var li = $('#docs li').filter(function() {
            return $(this).text().indexOf($('#ctl00_ctl01_docText').val()) === 0;
        });

        $('#ctl00_ctl01_docValue').val(li.attr('id'));
    }

    function search(event) {
        event.preventDefault();

        $('#ctl00_ctl01_textToSearch')[0].value = $('#ctl00_ctl01_textInput')[0].value;
        __doPostBack('ctl00_ctl01_Pesquisar','');
        startLoading();
    }

    function initBody() {
        setDocOnInit();
        
        $('.drop-list li').bind('click', bindDroplist);
        
        $("#docs li").on("click", search);
        $("#anos li").on("click", search);

        $('#BtnPesquisa').on("click", function (event) {
            event.preventDefault();

            var numCharacters = $('#ctl00_ctl01_textInput')[0].value.length;
            if(numCharacters >= minSearchCharacters || numCharacters == 0) {
                $('#ctl00_ctl01_textToSearch')[0].value = $('#ctl00_ctl01_textInput')[0].value;
                __doPostBack('ctl00_ctl01_Pesquisar','');
                startLoading();
            } else {
                alert('Apenas pode efectuar pesquisas com um mínimo de 3 caracteres.');
            }
        });

        $('nav.pagination ul li a').not('.current').on("click", function (event) {
            event.preventDefault();

            var textSelectedDoc = $('#ctl00_ctl01_docSelected').text();

            $('#docs li:contains("' + textSelectedDoc + '")').click();
            $('#ctl00_ctl01_currentResultsViewPage')[0].value = $(this).attr('href');

            __doPostBack('ctl00$ctl01$NovaPagina', '');
            startLoading();
        });

        $('.drop-style').each(function () {
            var dropText = $(this).find('.drop-text').val();
            var dropSelected = $(this).find('.drop-selected');
            dropSelected.html(dropText);
        });
    }

    function stopLoading() {
        $("body").removeClass("loading");
        $("#loading").css('display', 'none');
    }

    function startLoading() {
        $("#loading").css('display', 'block');
        $("body").addClass("loading");
    }
</script>

        
