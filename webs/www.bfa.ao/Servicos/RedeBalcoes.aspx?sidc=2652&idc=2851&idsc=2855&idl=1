<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false">
</script>
<script type="text/javascript">

    var infowindow;
//    var prm = Sys.WebForms.PageRequestManager.getInstance();
    var markers = [];
    var offset = 0.004;
    var minSearchCharacters = 3;

    

    $(function () {
        initEvents();
        initBody();
    }); 

//    prm.add_endRequest(function () {
//        //BFA.Main.init();
//        BFA.UI.selectStyling();
//        initEvents();
//        initBody();
//    });

    function clearMarkers() {
        for (var i = 0; i < markers.length; i++) {
            var marker = markers[i].value;
            marker.setMap(null);
        }
        markers = [];
    }

    function drawBranchMarkers() {
        $('.results-container').find('li').each(function () {
            var branchEntityID = $(this).find('.BranchEntityID').val();
            var branchName = $(this).find('a').text();
            var branchGeoLocationLatitude = $(this).find('.BranchGeoLocationLatitude').val();
            var branchGeoLocationLongitude = $(this).find('.BranchGeoLocationLongitude').val();
            var branchAddress = $(this).find('.BranchAddress').val();
            createMarker(branchEntityID,branchGeoLocationLatitude, branchGeoLocationLongitude, branchName, branchAddress);
        });
    }

    function createMarker(markerID, markerLatitude, markerLongitude, markerTitle, markerContent) {
        var marker = new google.maps.Marker({
            position: new google.maps.LatLng(markerLatitude, markerLongitude),
            map: map,
            icon: '/img/logos/bfa_logo_gmaps.png',
            title: markerTitle
        });
        markers.push({key:markerID, value:marker});
        iWindow(marker, markerContent);
    }

    function iWindow(marker, content) {
        google.maps.event.addListener(marker, 'click', function () {
            infowindow.setContent(content);
            infowindow.open(map, marker);
            centerMapBasedOnMarker(marker);
        });
    }

    function initBody() {
        $('.drop-style').each(function () {
            var dropText = $(this).find('.drop-text').val();
            var dropSelected = $(this).find('.drop-selected');
            dropSelected.html(dropText);
        });

        infowindow = new google.maps.InfoWindow();

        

        var currentMunicipioLocation = $('#ctl00_ctl01_selectedMunicipioGeoLocation')[0].value;
        if ((currentMunicipioLocation != "") && (currentMunicipioLocation != null)) {
            var splitMunicipioLocation = currentMunicipioLocation.split(',');
            map.setCenter(new google.maps.LatLng(splitMunicipioLocation[0],splitMunicipioLocation[1]));
        }
        drawBranchMarkers();
    }

    function startLoading() {
        $("#loading").css('display', 'block');
        $("body").addClass("loading"); 
    }

    function getMarker(markerID) {
        for (i = 0; i < markers.length ; i++) {
            if (markerID == markers[i].key) {
                return markers[i].value;
            }
        }
        return null;
    }

    function centerMapBasedOnMarker(marker) {
        var correctLatitudeForVisualization = parseFloat(marker.getPosition().lat()) + offset;
        var centerPoint = new google.maps.LatLng(correctLatitudeForVisualization, marker.getPosition().lng());
        map.setCenter(centerPoint);
    }

    function initEvents() {
        $("body").removeClass("loading"); 

        $("#tiposBalcoes li").on("click", function () {
            var obj = '.drop-style';
            $(this).parents(obj).find('.drop-selected').text($(this).text());
            $(this).parents(obj).find('.drop-text').val($(this).text());
            $('#ctl00_ctl01_balcaoValue')[0].value = ($(this).attr("id"));
            $('#ctl00_ctl01_selectedBalcalTypeEntityID')[0].value = ($(this).attr("id"));
        });

        $("#provincias li").on("click", function () {
            var obj = '.drop-style';
            $(this).parents(obj).find('.drop-selected').text($(this).text());
            $(this).parents(obj).find('.drop-text').val($(this).text());
            $(this).parents(obj).find('.drop-value').val($(this).attr("id"));
            $('#ctl00_ctl01_selectedProvinciaEntityID')[0].value = $(this).attr("id");
            __doPostBack('ctl00$ctl01$UpdateProvincias', '');
            startLoading();
        });

        $("#municipios li").on("click", function () {
            $('#ctl00_ctl01_selectedMunicipioEntityID')[0].value = $(this).attr("id");
        });

        $('.branch-search .link-button a').on("click", function () {
            clearMarkers();
            
            __doPostBack('ctl00$ctl01$NovaPesquisa', '');
            startLoading();
        });

        $('#SearchCombos').on("click", function () {
            
            if(!!$('#ctl00_ctl01_balcaoValue').attr("value") != false && !!$('#ctl00_ctl01_provinciaValue').attr("value") != false)
            {
                clearMarkers();
                __doPostBack('ctl00$ctl01$PesquisarCombos', '');
                startLoading();
            }
            else
            {
                alert("Preencha os campos obrigatórios");
            }
        });

        $('#SearchNomeBalcao').on("click", function () {
            if( $('#ctl00_ctl01_branch')[0].value.length >= minSearchCharacters )
            {
                $('#ctl00_ctl01_insertedBalcaoName')[0].value = $('#ctl00_ctl01_branch')[0].value;
                clearMarkers();
                __doPostBack('ctl00$ctl01$PesquisarNomeBalcao', '');
                startLoading();
            }
            else
            {
                alert('Apenas pode efectuar pesquisas com um mínimo de 3 caracteres.');
            }
        });

        $('.results-container').find('li a').on("click", function () {
            var branchEntityID = $(this).parent().find('.BranchEntityID').val();
            var branchAddress = $(this).parent().find('.BranchAddress').val();
            var currentMarker = getMarker(branchEntityID);

            centerMapBasedOnMarker(currentMarker);

            infowindow.open(map, currentMarker);
            infowindow.setContent(branchAddress);
        });

        
        $('nav.paginationSide a').not('.current').on("click", function (event) {
            event.preventDefault();

            $('#ctl00_ctl01_currentResultsViewPage')[0].value = $(this).attr('href');

            if(($('#ctl00_ctl01_selectedProvinciaEntityID')[0].value != "") && ($('#ctl00_ctl01_selectedProvinciaEntityID')[0].value != null))
            {
                __doPostBack('ctl00$ctl01$NovaPagina', '');
            }
            else
            {
                __doPostBack('ctl00$ctl01$NovaPaginaNome', '');
            }
            
            startLoading();
        });

        $("#loading").css('display', 'none');
    }
    

    

</script>


        
<!-- Init Footer -->
