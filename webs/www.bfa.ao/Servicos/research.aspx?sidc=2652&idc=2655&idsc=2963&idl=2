<script type="text/javascript">

    popinURL = "/Servicos/PopupSubscription.aspx?idl=2";

    $(function () {
        initEvents();
    }); 

    function initEvents() {
        stopLoading();

        $("#drop_meses li").on("click", function () {
            startLoading();
            var obj = '.drop-style';
            $(this).parents(obj).find('.drop-value').val($(this).text());
            $("#drop_anos li").parents(obj).find('.drop-value').val($("#drop_anos li").parents(obj).find('.drop-selected').text());
            __doPostBack('ctl00_ctl01_UpdateButton', '');
            
        });

        $("#drop_anos li").on("click", function () {
            startLoading();
            var obj = '.drop-style';
            $(this).parents(obj).find('.drop-value').val($(this).text());
            $("#drop_meses li").parents(obj).find('.drop-value').val($("#drop_meses li").parents(obj).find('.drop-selected').text());
            __doPostBack('ctl00_ctl01_UpdateButton', '');
            
        });

        $("#loading").css('display', 'none');

    }

    function stopLoading() {
        $("body").removeClass("loading");
        $("#loading").css('display', 'none');
    }

    function startLoading() {
        $("#loading").css('display', 'block');
        $("body").addClass("loading");
    }

</script>


        
